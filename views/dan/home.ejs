    <body>
        <div id="map"></div>

        <div class="row">
            <div class="col-lg-8 col-10 search-bar">
                <i class="fa-solid fa-magnifying-glass search-bar__icon"></i>
                <input class="search-bar__input" type="text" placeholder="Tìm kiếm theo địa chỉ" />
            </div>
        </div>

        <!-- ad location -->
        <% for (let i = 0; i < adLocations.length; i++) { %>
        <div
            class="ads-point__info<%= i %>"
            data-id="<%= adLocations[i]._id %>"
            data-lat="<%= adLocations[i].latlng.lat %>"
            data-lng="<%= adLocations[i].latlng.lng %>"
            data-violate="<%= adLocations[i].isViolated %>"
        >
            <div class="info__form"><%= adLocations[i].adType.name %></div>
            <div class="info__type"><%= adLocations[i].type.name %></div>
            <div class="info__address"><%= adLocations[i].address %></div>
            <img class="info__img" src="<%= adLocations[i].image.url %>" alt="" />
            <div class="info__planning-stt"><%= adLocations[i].status %></div>
        </div>
        <% } %>

        <!-- violated point latlng -->
        <% for (let i = 0; i < violatedPoints.length; i++) { %>
        <div
            class="violated-point__latlng<%= i %>"
            data-id="<%= violatedPoints[i]._id %>"
            data-lat="<%= violatedPoints[i].latlng.lat %>"
            data-lng="<%= violatedPoints[i].latlng.lng %>"
            hidden
        ></div>
        <% } %>

        <!-- toggle -->
        <div class="row">
            <div class="col-10 col-lg-11 toggle-section">
                <ul class="toggle-list">
                    <li class="toggle-item">
                        <label class="toggle-switch">
                            <input type="checkbox" checked />
                            <span class="toggle-slider"></span>
                        </label>
                        <span class="toggle-label">Bảng QC</span>
                    </li>
                    <li class="toggle-item">
                        <label class="toggle-switch">
                            <input type="checkbox" checked />
                            <span class="toggle-slider"></span>
                        </label>
                        <span class="toggle-label">Báo cáo vi phạm</span>
                    </li>
                </ul>
            </div>
        </div>

